<record>
  <name>Ammunition</name>
  <fields>
    <editor-id />
    <object-bounds />
    <full-name />
    <models />
    <pickup-sound />
    <drop-sound />
    <description />
    <keyword-list />
    <field>
      <key>DATA</key>
      <name>Data</name>
      <struct>
        <name>AmmunitionData</name>
        <members>
          <member>
            <name>Projectile</name>
            <type>uint</type>
            <as>
              <type>ref(PROJ)</type>
            </as>
          </member>
          <ammo-flags name="Flags" />
          <float name="Damage" />
          <uint name="Value" display-name="Gold Value" />
        </members>
      </struct>
    </field>
    <string key="ONAM" name="ShortName" hidden="true" />
  </fields>
</record>
